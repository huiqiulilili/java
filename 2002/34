<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
    <input type="button" value="interval" onclick="clear1();">
    <input type="button" value="timeout" onclick="clear2();">
    <script>
        var id1 =  setInterval("alert('123');",3000);
        var id2 =  setTimeout("alert('asd');",3000);
        function clear1(){
            clearInterval(id1);
        }
        function clear2(){
            clearTimeout(id2);
        }
    </script>
</body>
</html>
