    <script>
    document.write("<table border = '1' border-color = 'blue'>");
        for(var i = 1;i <= 9;i++){
            document.write("<tr>");
                for(var j = 1;j <= i; j ++) {
                    document.write("<td>");
                        document.write(j + "*" + i + "=" + i*j);
                    document.write("</td>");
                }
<!--            document.write("<br>");-->
            document.write("</tr>");
        }
    document.write("</table>");

    </script>
