<body>

        <ul id="ulid41">
            <li id="li31">王俊凯</li>
            <li id="li32">王源</li>
            <li id="li33">易烊千玺</li>
        </ul>

    <input type="button" value="replace" onclick="replace1()">
    <script>
        function replace1(){
            var li33 =  document.getElementById("li33");
            var li34 =  document.createElement("li");
            var text34 =  document.createTextNode("鹿晗");
            li34.appendChild(text34);
            var ulid41 =  document.getElementById("ulid41");
            ulid41.replaceChild(li34,li33);
        }
    </script>
