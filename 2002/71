<body>
    <ul id="ulid">
        <li>1111</li>
        <li>2222</li>
        <li>3333</li>
        <li>4444</li>
    </ul>
    <br>
    <input type="button" value="add" onclick="add1()">
    <script>
        function add1(){
            var ul1 =  document.getElementById("ulid");

            var li1 =  document.createElement("li");

            var text1 =  document.createTextNode("5555");

            li1.appendChild(text1);

            ul1.appendChild(li1);
        }
    </script>
</body>
</html>
