        var arr17 = ["张三","李素","万给"];
        document.write("原始数组：" + arr17);
        document.write("<br>");
        document.write("return:"+arr17.pop());
        document.write("<br>");
        document.write("新数组：" + arr17);
        document.write("<hr>");


        var arr18 = ["a","b","c","d"];
        document.write("原始数组：" + arr18);
        document.write("<br>");
        document.write("新数组：" + arr18.reverse());



    </script>
