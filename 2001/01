package 复习;

import java.io.FileReader;
import java.io.IOException;

public class FileReaderDemo {
    public static void main(String[] args) throws IOException {
        FileReader fr = new FileReader("a.txt");
        int by = 0;
        while ((by = fr.read()) != -1) {
            System.out.print((char) by);
        }
    }
}
