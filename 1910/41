import java.util.Random;
import java.util.Scanner;

public class Practice05 {

    public static void origin(){
        Random random = new Random(20190806);
        System.out.println("请输入第一个数组的行数：");
        int low1 = random.nextInt(100);
        System.out.println("请输入第一个数组的列数：");
        int column1 = random.nextInt(100);
        System.out.println("请输入第二个数组的行数：");
        int low2 = random.nextInt(100);
        System.out.println("请输入第二个数组的列数：");
        int column2 = random.nextInt(100);
        int[][] array1 = new int[low1][column1];
        int[][] array2 = new int[low2][column2];
    }
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        origin();
    }
}
