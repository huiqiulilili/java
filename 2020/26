package JDK5新特性;

import java.util.Arrays;
import java.util.List;

public class AsArrayDemo {
    public static void main(String[] args) {
        String[] array = {"hello","world","Java"};

        List<String> list = Arrays.asList(array);

        for(String s : list) {
            System.out.println(s);
        }
    }
}
